{
  "name": "rob.ac",
  "version": "1.0.0",
  "description": "My personal website.",
  "main": "index.js",
  "scripts": {
    "build-css-step:compile-scss": "node-sass --source-map true --output-style compressed src/scss -o css",
    "build-css-step:add-vendor-prefixes": "postcss css/**/*.css --use autoprefixer --replace",
    "build:css": "npm-run-all build-css-step:*",
    "build": "npm-run-all -p build:*",
    "watch:sass": "chokidar 'src/scss/**/*.scss' -c 'npm run build:css'",
    "serve": "live-server .",
    "dev": "npm-run-all -p serve watch:sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeparateRecords/SeparateRecords.github.io.git"
  },
  "keywords": [],
  "author": "SeparateRecords",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SeparateRecords/SeparateRecords.github.io/issues"
  },
  "homepage": "https://github.com/SeparateRecords/SeparateRecords.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "chokidar-cli": "^2.1.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
