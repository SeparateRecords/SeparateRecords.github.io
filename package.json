{
  "name": "rob.ac",
  "version": "1.0.0",
  "description": "My personal website.",
  "main": "index.js",
  "scripts": {
    "build-scss-step:compile-scss": "node-sass --source-map=true --output-style=compressed src/scss -o css",
    "build-scss-step:add-vendor-prefixes": "postcss css/*.css css/**/*.css --verbose --replace --use autoprefixer",
    "build:scss": "npm-run-all build-css-step:*",
    "build": "npm-run-all -p build:*",
    "watch:scss": "chokidar 'src/scss/**/*.scss' -c 'npm run build:scss'",
    "server": "live-server . --ignorePattern='^(CNAME|package.*|README.md|.gitignore|.git|src)' --wait=500",
    "dev": "npm-run-all -p server watch:scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeparateRecords/SeparateRecords.github.io.git"
  },
  "keywords": [],
  "author": "SeparateRecords",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SeparateRecords/SeparateRecords.github.io/issues"
  },
  "homepage": "https://github.com/SeparateRecords/SeparateRecords.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "chokidar-cli": "^2.1.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
